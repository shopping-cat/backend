{
  "name": "back",
  "scripts": {
    "dev": "ts-node-dev --notify --respawn --transpile-only src/server.ts",
    "migrate": "npm -s run migrate:save && npm -s run migrate:up",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "start": "cross-env NODE_ENV=production node dist/server",
    "clean": "rm -rf dist",
    "migrate:save": "prisma migrate save --name init --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only src/schema",
  },
  "dependencies": {
    "@nexus/schema": "^0.19.2",
    "@prisma/client": "^2.12.1",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.20.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "migrate": "^1.7.0",
    "morgan": "^1.10.0",
    "nexus-plugin-prisma": "^0.25.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@prisma/cli": "^2.12.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.4",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62"
  }
}