{
  "name": "back",
  "scripts": {
    "dev": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only src/server.ts",
    "dev:migrate": "prisma migrate dev --preview-feature",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus:app && npm -s run generate:nexus:admin && npm -s run generate:nexus:partner",
    "studio": "prisma studio",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "start": "cross-env NODE_ENV=production node dist/server",
    "clean": "rm -rf dist",
    "dev:migrate:save": "prisma migrate save --experimental",
    "dev:migrate:up": "prisma migrate up --experimental",
    "generate:prisma": "prisma generate",
    "generate:nexus:app": "cross-env NODE_ENV=development ts-node --transpile-only src/schemas/app",
    "generate:nexus:admin": "cross-env NODE_ENV=development ts-node --transpile-only src/schemas/admin",
    "generate:nexus:partner": "cross-env NODE_ENV=development ts-node --transpile-only src/schemas/partner"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.0",
    "@prisma/client": "2.14.0",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.1",
    "graphql": "^15.4.0",
    "graphql-scalars": "^1.7.0",
    "graphql-upload": "^11.0.0",
    "helmet": "^4.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "migrate": "^1.7.0",
    "morgan": "^1.10.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.28.0",
    "typescript": "^4.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.4",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.10.1",
    "@types/uuid": "^8.3.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62"
  }
}
